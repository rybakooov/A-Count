section.Price(class=blueFooter ? 'Price_blue_80' : '')
  .container
    .Price-Head
      h3.Price-Head__Title Сколько это будет <br>стоить для Вас
      p.Price-Head__Text Ответьте на вопросы и мы подскажем какой тарифный план подходит именно Вам, а так же дадим&nbsp;
        span скидку <br>на первый месяц пользования
    .Price-Quiz
      .Price-Quiz-Control
        button.Price-Quiz-Control__Prev
          <svg width="8" height="5" viewBox="0 0 8 5" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path d="M1 1L4 4L7 1" stroke="#898989" stroke-linecap="round" stroke-linejoin="round"></path>
          </svg>
        p.Price-Quiz-Control__State
          span.Price-Quiz-Control__State_cur 1
          | &nbsp;/&nbsp;
          span.Price-Quiz-Control__State_all
      .Price-Quiz-Wrap
        .Price-Quiz-Question.Price-Quiz-Question_current
          p.Price-Quiz-Question__Title Какой объем документации будет обрабатываться онлайн?
          .Price-Quiz-Answers
            input(type="radio" name="firstRow" id="11" value="1 раз в месяц")
            label(for="11").Price-Quiz-Answers-Label
              .Price-Quiz-Answers-Label__Flag
              p.Price-Quiz-Answers-Label__Value 1 раз в месяц
            input(type="radio" name="firstRow" id="12" value="2 раз в месяц")
            label(for="12").Price-Quiz-Answers-Label
              .Price-Quiz-Answers-Label__Flag
              p.Price-Quiz-Answers-Label__Value 2 раз в месяц
            input(type="radio" name="firstRow" id="13" value="3 раз в месяц")
